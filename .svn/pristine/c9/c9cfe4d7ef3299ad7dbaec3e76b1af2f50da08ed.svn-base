textarea {padding:10px; border-color:#cccccc; box-sizing:border-box;}
.textarea_box {position:relative; float:left;height:120px; width:100%;border:1px solid #ddd; box-sizing:border-box;}
.textarea_box .count_bite {position:absolute; right:20px; bottom:10px; font-size:15px; color:#999;}
.textarea_box .count_bite em {color:#222;}
.textarea_box textarea {height:80px; border:none;}

button { font-size:15px; letter-spacing:-1px;}

	/* ch */
label.ch {display: inline-block; line-height: 24px; margin: 0 14px 0 0; cursor: pointer; vertical-align: middle;}
label.ch span {display: inline-block;}
label.ch input[type="radio"]:not(o) + span:before,
label.ch input[type="checkbox"]:not(o) + span:before {content: ' '; display: inline-block; margin-right:6px; margin-bottom:2px; width: 24px;height: 24px; text-align: center; border:1px solid #cccccc;  vertical-align: middle; box-sizing:border-box; }
label.ch input[type="radio"]:not(o) + span:before {border-radius: 50%;}
label.ch input[type="checkbox"]:not(o) + span:before {background: url(../../images/ch_chbox.png) no-repeat 50% 50%;}
label.ch input[type="radio"]:not(o),  
label.ch input[type="checkbox"]:not(o) {width: 0; padding: 0; opacity:0; -webkit-appearance: none; overflow: hidden; border: 0}
label.ch input[type="radio"]:checked + span:before {background: url(../../images/ch_radio_ch.png) no-repeat 50% 50%; border-color:#3b8c9a; }
label.ch input[type="checkbox"]:checked + span:before {border-color:#3b8c9a; background-color:#3b8c9a;  background-image: url(../../images/ch_chbox_ch.png); }

.td_ch label.ch {margin:0;}
.td_ch label.ch input[type="radio"]:not(o) + span:before,
.td_ch label.ch input[type="checkbox"]:not(o) + span:before {margin:0; vertical-align: top;}

label.ch + label.ch {margin-left:10px;}
input[type="text"] + .ch {margin-left:10px;}

.ch_list li {display:inline-block; float:left; padding:5px 0; width:24.5%; }
.ch_list.list5 li {width:20%}
.ch_list.list6 li {width:16.6%}
.ch_list li label.ch {white-space:nowrap;}

.alignC,
.tb td.alignC {text-align:center;}


.fl {float:left;}
.fr {float:right;}

h3.blind,
h4.blind,
h5.blind {display:none;}

