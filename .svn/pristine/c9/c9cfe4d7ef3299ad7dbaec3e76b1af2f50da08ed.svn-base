@charset "UTF-8";

/* common style */
a {cursor:pointer;}

select {padding: 0 28px 0 10px; padding-right:0  \0/IE8+9; height:40px; min-width:45px; line-height:38px; color:#555;border:1px solid #ccc; -webkit-appearance: none; -moz-appearance: none; appearance: none; outline: none;  background:url(../../images/ico_slt_arow.png) no-repeat 100% 50% #fff;  background-size:auto 7px ; box-sizing:border-box;}
select::-ms-expand { display: none; }
select.slt_s {min-width:45px;}

input[type="text"], 
input[type="password"]{padding:0 10px; height:38px; min-width:50px; line-height:36px; border:1px solid #cccccc; box-sizing:border-box;}
input[type="text"][readonly] {background:#fff;}
input::-webkit-input-placeholder { color:#aaa }
input::-moz-placeholder { color:#aaa }
input::-ms-input-placeholder { color:#aaa }

