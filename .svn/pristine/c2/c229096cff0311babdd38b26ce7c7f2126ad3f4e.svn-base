@charset "UTF-8";

/* Frame */
body {}
.wrapper {position:relative; width:100%; min-width:1140px;min-height:100%; background:#fff; overflow:hidden;}
.wrapper.main {height:100%; min-height:1000px;}
#header {z-index:500; position:relative; height:100px; border-bottom:1px solid #c8c8c8; background:#fff; box-sizing: border-box;}
#idx_contents {margin:0 auto;} 
#contents {position:relative; margin:0 auto; padding:0 40px 80px; min-height:700px; max-width:1200px;}
.main #contents {padding-bottom:330px; height:100%; min-height:100%;  box-sizing:border-box;}
.sub #contents {margin:0 auto 150px; width:1100px; min-height:650px; padding-bottom:100px;}
#footer {z-index:499; position:absolute; bottom:0; width:100%; height:80px; background:#fff;  border-top:1px solid #e1e1e1;}

/* header */
.hd_w {z-index:100;margin:0 auto; position:relative; height:100px; }

.hd_w h1 {z-index:101; display:block; position:relative; float:left; margin-left:40px; height:100px;}
.hd_w h1 a {display:block; position:relative; top:50%; margin-top:-13px; }
.hd_w h1 a img { vertical-align:top;}

/* global navigation */
.hd_gnb {float:left; margin-left:60px;}
.hd_gnb ul.dep1_list  {white-space:nowrap;}
.hd_gnb ul.dep1_list li {display:block;}
.hd_gnb ul.dep1_list li a {display:block; }
.hd_gnb ul.dep1_list li a span {display:block; position:relative;}
.hd_gnb ul.dep1_list > li {float:left;}
.hd_gnb a.dep1 { font-size:22px; font-weight:500; color:#222; }
.hd_gnb a.dep1 span {margin:0 30px; padding:0 3px; height:100px; line-height:100px;}
.hd_gnb a.dep1:hover {color:#0078ae;}
.hd_gnb a.dep1 span:after {content:''; display:block; position:absolute; left:0; bottom:0; width:0; height:4px; background:#0078ae; webkit-transition: all 0.3s; transition: all 0.3s;}
.hd_gnb a.dep1:hover span:after {width:0; }
.hd_gnb .ac a.dep1 {  color:#045fa5;}
.hd_gnb .ac a.dep1 span:after,
.hd_gnb a.dep1.act span:after,
.hd_gnb .act a.dep1 span:after { width:100%;}

.hd_sgnb {z-index:101;position:absolute; right:20px; top:50%; margin-top:-13px;}
.hd_sgnb a {display:block; position:relative;float:left; padding:5px 20px; font-size:14px;  }
.hd_sgnb a:before {content:''; display: block; position:absolute; left:-1px; top:50%; margin-top:-5px; width:2px; height: 12px; background:#ccc; }
.hd_sgnb a:first-child:before {display:none;}
.hd_sgnb a.btn_login span {display:block;width:75px; height:75px; background:url(../../images/hd_icon.png) no-repeat 50% 50%;}


/* footer */
.footer .c {position:relative; top:50%; margin-top:-10px; text-align:center; color:#888888; font-weight:300; font-size:13px; letter-spacing: 0;}




